{
  "name": "sky-messanger",
  "version": "1.12.3",
  "description": "Just a simple messanger",
  "main": "index.js",
  "author": "Bykovskikh Evgeny <redzumi.tao@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --hot --config config/webpack/dev.webpack.config.js --history-api-fallback",
    "build:prod": "cross-env NODE_ENV=production && webpack --config config/webpack/prod.webpack.config.js --history-api-fallback",
    "prestart:prod": "babel src --presets=@babel/env,@babel/preset-react --plugins=@babel/plugin-proposal-class-properties,babel-plugin-css-modules-transform --no-babelrc --out-dir build",
    "start:prod": "cd build && node server.js",
    "prestart:api": "babel src/api.js --presets=@babel/env --no-babelrc --out-dir build",
    "start:api": "cd build && node api.js",
    "ci:prod": "rm -rf build && npm run build:prod && npm run start:prod",
    "ci:dev": "rm -rf build && npm run start",
    "lint": "eslint --ext .js --ext .jsx src"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "axios": "^0.18.0",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "body-parser": "^1.18.3",
    "bulma": "^0.7.1",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "cross-env": "^5.2.0",
    "express": "^4.16.3",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "progress-bar-webpack-plugin": "^1.11.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "style-loader": "0.23.1",
    "styled-components": "^4.1.2",
    "uuid": "^3.3.2",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2",
    "write-file-webpack-plugin": "^4.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.0.0-beta.52",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.52",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/preset-react": "^7.0.0-beta.51",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.0-beta",
    "css-loader": "^1.0.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "react-hot-loader": "^4.3.3",
    "webpack-dev-server": "^3.1.4"
  }
}
